pipeline {
    agent any
    stages {
        stage('login server'){
         steps{
            sshagent(credentials:['ssh-credentials-id']){
               sh 'ssh  -o StrictHostKeyChecking=no  root@161.97.136.69 uptime "whoami"'
          }
        echo "success lgoin"
         }
       }
    }
    post {
        always {
            cleanWs()
        }
        success {
            echo 'Build successful!'
        }
        failure {
            echo 'Build failed!'
        }
    }
}
